{
  "version": 2,
  "builds": [
    {
      "src": "api/create-payment-intent.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/**/*",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/create-payment-intent.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.html"
    }
  ]
}